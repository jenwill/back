{
  "config": {
    "target": "http://sockit.live:3000",
    "phases": [
      {
        "duration": 10,
        "arrivalRate": 50
      }
    ]
    ,
    "processor" : "./load-test-create-user.js"
  },
  "scenarios": [
    {
      "name" : "Create Users",
      "flow": [
        {
          "function": "create"
        },
        {
          "post": {
            "url": "/api/v1/register",
            "json": {
              "username": "{{ username }}",
              "password": "{{ password }}"
            }
          }
        }
      ]
    }
  ]
}
